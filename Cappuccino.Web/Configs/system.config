<?xml version="1.0" encoding="utf-8"?>
<appSettings>
	<add key="webpages:Version" value="3.0.0.0" />
	<add key="webpages:Enabled" value="false" />
	<add key="ClientValidationEnabled" value="true" />
	<add key="UnobtrusiveJavaScriptEnabled" value="true" />
	<!-- ================== 1：log4net日志相关配置 ================== -->
	<add key="log4net.Config" value="Configs/log4net.config"/>
	<add key="log4net.Config.Watch" value="True" />
	<!-- ================== 2：开发系统相关配置 ================== -->
	<!--初始化密码-->
	<add key="InitUserPwd" value="123456" />
	<!--上传文件大小-->
	<add key="UploadFileByImgSize" value="2048" />
	<!--上传文件类型-->
	<add key="UploadFileByImgType" value="png|gif|jpg|jpeg" />
	<!--数据库备份文件保存路径-->
	<add key="DbBackupPath" value="" />

    <!--可选值:"HttpRuntime"(默认),"Memory","Redis"-->
    <add key="CacheType" value="HttpRuntime" />

    <!-- 压缩包/文件配置 -->
    <!-- 支持的压缩包格式 -->
    <add key="CompressSupportFormats" value="zip,rar,7z" />
    <!-- 支持的文件格式 -->
    <add key="FileSupportFormats" value="jpg,png" />
    <!-- 100MB -->
    <add key="UploadMaxFileSize" value="104857600" />
    
    <!-- 目录配置 -->
    <add key="CompressTempPath" value="~/Resource/Upload/CompressTemp/" />
    <add key="UnzipTempPath" value="~/Resource/Upload/UnzipTemp/" />
    <!-- 获取物理路径（转换虚拟路径）-->
    <add key="ValidFilePath" value="~/Resource/Upload/ValidFiles/" /> 
    <!-- AI服务配置 -->
    <add key="AIService" value="http://localhost:8080/owenvl8b/api/recognize" />
    <add key="AITimeout" value="30000" />
</appSettings>